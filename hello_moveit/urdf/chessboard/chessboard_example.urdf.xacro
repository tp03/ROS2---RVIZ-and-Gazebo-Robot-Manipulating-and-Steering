<robot  xmlns:xacro="http://www.ros.org/wiki/xacro"
        name="chessboard" >
  <!-- Example on how to include the chessboard for eye-hand calibration -->
  <xacro:include filename="$(find pal_gazebo_worlds)/urdf/chessboard/chessboard.urdf.xacro" />
  <xacro:chessboard name="arm_right_chessboard" target_model="talos" target_link="arm_right_7_link" pose="0.15 0.1 -0.22 0.0 0.0 -1.57"/>
</robot>

